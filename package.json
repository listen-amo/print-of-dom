    "build:module": "webpack --config=./config/module.webpack.config.js",
    "build:web": "webpack --config=./config/web.webpack.config.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "babel-loader": "^8.2.3",
    "core-js": "^3.20.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  }
